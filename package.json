{
  "name": "nauticus-website",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "prepare": "husky install",
    "prettier": "prettier \"**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}\"",
    "check:format": "npm run prettier -- --check",
    "check:types": "tsc --project tsconfig.json",
    "check:spelling": "cspell --show-context --no-progress \"**\"",
    "check:lint": "next lint",
    "fix:format": "npm run prettier -- --write",
    "fix:lint": "npm run check:lint -- --fix",
    "validate": "FORCE_COLOR=1 npm-run-all --parallel --aggregate-output --print-label check:*"
  },
  "dependencies": {
    "@emotion/react": "^11.7.1",
    "@emotion/styled": "^11.6.0",
    "@mui/icons-material": "^5.3.0",
    "@mui/material": "^5.3.0",
    "@mui/styles": "^5.3.0",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "next": "^12.0.8",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "cspell": "^5.16.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "^12.0.8",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "husky": "^7.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "typescript": "^4.5.4"
  }
}
